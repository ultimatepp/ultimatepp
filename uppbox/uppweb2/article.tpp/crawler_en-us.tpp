[s1; -|-|-|-|`}&]
[s1; -|-|-|-|http.Remove(i); // remove from active requests&]
[s1; -|-|-|-|work.Remove(q); // remove from GUI list of active requests&]
[s1; -|-|-|`}&]
[s1; -|-|`}&]
