[s0;# [2 Eigen is very easy to use. For example, to multiply two matrices:]&]
[s0;#2 &]
[s0;# [C2 -|Matrix2d a, b;]&]
[s0; [C2 -|a << 1, 2,-|-|-|// Fill the data]&]
[s0; [C2 -|-|3, 4;]&]
[s0; [C2 -|b << 5, 6,]&]
[s0; [C2 -|-|7, 8;]&]
[s0; [C2 -|Matrix2d res `= a`*b;-|// Just multiply them using `*]&]
[s0;#2 &]
