[s7; -|-|std`::cerr << `"Hello, platform! `" << std`::endl;&]
[s7; -|-|std`::cin >> dummy;&]
[s7; -|-|&]
[s7; -|-|return 0;&]
[s7; -|`}&]
[s7; // `-`- example `-`-&]
[s5; &]
